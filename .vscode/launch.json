{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug QtOcctViewer",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/Debug/QtOcctViewer.exe",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [
                {
                    "name": "PATH",
                    "value": "C:/Qt/6.8.1/mingw_64/bin;C:/Qt/Tools/mingw1310_64/bin;${env:PATH}"
                }
            ],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "CMake Build",
            "miDebuggerPath": "C:/Qt/Tools/mingw1310_64/bin/gdb.exe"
        }
    ]
}
